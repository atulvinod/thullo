{
    "name": "nodeboilerplate",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "node bin/server.js",
        "cluster": "node bin/cluster.js",
        "test": "npx jest tests/unit/*",
        "test:coverage": "npm test -- --coverage",
        "test:ci": "npm test -- --ci --coverage",
        "schema:generate": "node ./tests/unitTestData/db/util/generateSchema.js",
        "migration:create": "npm run schema:generate -- migration -o create --table",
        "migration:alter": "npm run schema:generate -- migration -o alter --table",
        "seed:generate": "npm run schema:generate -- seeder --table"
    },
    "dependencies": {
        "axios": "^0.21.4",
        "axios-retry": "^3.1.9",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "dateformat": "^4.5.1",
        "dotenv": "^16.3.1",
        "express": "^4.17.1",
        "file-stream-rotator": "^0.5.7",
        "firebase": "^10.3.0",
        "http-errors": "^1.8.0",
        "http-status-codes": "^2.2.0",
        "ip": "^1.1.5",
        "jsonwebtoken": "^9.0.1",
        "knex": "^0.95.14",
        "lodash": "^4.17.21",
        "log4js": "^6.3.0",
        "module-alias": "^2.2.2",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^2.2.5",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "pg": "^8.11.3",
        "prom-client": "^13.1.0",
        "pug": "^3.0.2",
        "redis": "^3.1.2",
        "resend": "^3.2.0",
        "stack-trace": "^0.0.10",
        "uuid": "^8.3.2",
        "validator": "^13.7.0"
    },
    "devDependencies": {
        "eslint": "^8.4.1",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.25.3",
        "jest": "^26.6.3",
        "jest-junit": "^12.2.0",
        "npm-check-updates": "^11.8.3",
        "yargs": "^17.0.1"
    },
    "_moduleAliases": {
        "@util": "util",
        "@services": "services",
        "@lib": "lib",
        "@config": "config",
        "@errors": "errors",
        "@models": "models",
        "@src": "src"
    }
}
